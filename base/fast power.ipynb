     "start_time": "2025-05-10T13:51:08.111068Z"
    }
   },
   "source": [
    "def fast_pow(a: float, n: int) -> float:\n",
    "    temp = 1.0\n",
    "    if n < 0:\n",
    "        return 1 / fast_pow(a, -n)\n",
    "    while n:\n",
    "        if n & 1:\n",
    "            temp *= a\n",
    "        a *= a\n",
    "        n >>= 1\n",
    "    return temp"
   ],
   "outputs": [],
